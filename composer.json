{
  "type": "symfony-bundle",
  "name": "crovitche/swiss-geo-bundle",
  "description": "Import swiss building addresses, streets and localities locally on your MySQL database using Doctrine ORM",
  "keywords": ["doctrine", "addresses", "orm", "symfony-ux"],
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Thibault Gattolliat",
      "email": "contact@thibaultg.info",
      "homepage": "https://contact@thibaultg.info",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.2",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.13 || ^3.0",
    "guzzlehttp/guzzle": "^7.7",
    "http-interop/http-factory-guzzle": "^1.0",
    "league/csv": "^9.7",
    "meilisearch/meilisearch-php": "^1.5",
    "symfony/cache": "^6.4|^7.0",
    "symfony/config": "^6.4|^7.0",
    "symfony/console": "^6.4|^7.0",
    "symfony/dependency-injection": "^6.4|^7.0",
    "symfony/filesystem": "^6.4|^7.0",
    "symfony/finder": "^6.4|^7.0",
    "symfony/http-client": "^6.4|^7.0",
    "symfony/http-foundation": "^6.4|^7.0",
    "symfony/form": "^6.4|^7.0",
    "symfony/monolog-bundle": "^3.7",
    "symfony/options-resolver": "^6.4|^7.0",
    "symfony/lock": "^6.4|^7.0",
    "symfony/yaml": "^6.4|^7.0"
  },
  "suggest": {
    "ext-curl": "*"
  },
  "require-dev": {
    "jetbrains/phpstorm-attributes": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Crovitche\\SwissGeoBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Crovitche\\SwissGeoBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
